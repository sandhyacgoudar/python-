a1={"Red","Blue","Green",240}
b1={"Birds","Insects",0.89,"@","Red"}
c1={100,500,50,"Ram"}
print(a1|c1) 
print(a1.intersection(b1)) 
print(a1-b1) 
print(a1.symmetric_difference (b1)) 
c1.add(300) 
print(c1) 
b1.discard("Birds") 
print(b1) 
a1.clear() 
print(a1) 
b1.pop() 
print(b1) 
print(b1.remove(0.89)) 

